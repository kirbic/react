{
  "compilerOptions": {
    "target": "es2020",
    "module": "ESNext",
    "strict": true,
    "lib": ["es2020", "DOM"],
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"]
}
